{
	"manifest_version": 2,
	"name": "Rbhd",
	"version": "1.0",
	"permissions": [
		"activeTab",
		"tabs",
		"http://*/",
		"https://*/*",
		"storage",
		"contextMenus"
	],
	"browser_action": {
		"default_icon": "img/icon48.png",
		"default_popup": "build/index.html",
		"default_title": "Robinhood"
 	},
 	"icons": {
 		"16": "img/icon16.png",
 		"48": "img/icon48.png",
 		"128": "img/icon128.png"
 	},
 	"background": {
		"page": "build/index.html"
    }
}